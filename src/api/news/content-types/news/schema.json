{
  "kind": "collectionType",
  "collectionName": "newss",
  "info": {
    "singularName": "news",
    "pluralName": "newss",
    "displayName": "News"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Article": {
      "type": "blocks",
      "required": true
    },
    "Picture": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "news"
    }
  }
}
